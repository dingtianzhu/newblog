{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": "",
    "paths": {
      "@/*": ["./src/*"],
      "@img/*": ["./src/assets/img/*"],
      "@svg/*": ["./src/assets/svg/*"],
      "@libs/*": ["./src/libs/*"],
      "@plugins/*": ["./src/plugins/*"],
      "@cp/*": ["./src/components/*"],
      "@views/*": ["./src/views/*"]
    },
    "lib": ["DOM"]
  }
}
